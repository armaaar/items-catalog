{% extends "./head/head.jinja" %}

{% block content %}

<div class="container">
    <div class="col-md-4 col-xs-12">
        <h3>Categories</h3>
        <ul class="side-bar">
            {% for category in categories %}
                <li id="{{category.id}}">
                    <a href="javascritp:void(0)">{{category.name}}</a>
                </li>
            {% endfor %}
        </ul>
    </div>
    <div class="col-md-8 col-xs-12">
        <h3>Latest Items</h3>
        <ul class="side-bar">
            {% for item in items %}
                <li id="{{item.id}}">
                    <a href="javascritp:void(0)">{{item.name}}</a>
                </li>
            {% endfor %}
        </ul>
    </div>
</div>
{% endblock content %}
